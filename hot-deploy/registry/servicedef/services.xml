		<override name="Folio" optional="false" />
		<override name="retentionScheduleName" optional="false" />
		<override name="retentionScheduleStatus" optional="false" />
	</service>
	<service name="updateRetentionSchedule" default-entity-name="DocumentRetentionSchedule"
