		<auto-attributes entity-name="IssueFile" include="pk" mode="IN" optional="true" />
		<auto-attributes entity-name="IssueFile" include="nonpk" mode="IN" optional="true" />
	</service>
	<service name="updateIssueFile" default-entity-name="IssueFile"
		engine="simple" location="component://registry/script/org/ofbiz/registry/IssueFileServices.xml" 
		invoke="updateIssueFile" auth="true">
		<description>Update a File Request</description>
		<permission-service service-name="partyBasePermissionCheck"
			main-action="UPDATE" />
		<auto-attributes include="pk" mode="IN" optional="false" />
		<auto-attributes include="nonpk" mode="IN" optional="true" />
	</service>

		<!-- Release File services -->
	<service name="createReleaseFile" engine="simple"
		location="component://registry/script/org/ofbiz/registry/ReleaseFileServices.xml"
		invoke="createReleaseFile" auth="true">
		<description>Create File Request header record</description>
		<auto-attributes entity-name="ReleaseFile" include="pk" mode="IN" optional="true" />
		<auto-attributes entity-name="ReleaseFile" include="nonpk" mode="IN" optional="true" />
	</service>
	<service name="updateReleaseFile" default-entity-name="ReleaseFile"
