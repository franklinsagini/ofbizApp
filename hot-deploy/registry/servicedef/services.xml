	<service name="updateFileStatus" default-entity-name="Files" engine="simple"
            location="component://registry/script/org/ofbiz/registry/FileServices.xml" invoke="updateFileStatus" auth="true">
        <description>Update a file's status</description>
        <auto-attributes mode="IN" include="pk" optional="false"/>
        <auto-attributes mode="IN" include="nonpk" optional="true"/>
    </service>
	
