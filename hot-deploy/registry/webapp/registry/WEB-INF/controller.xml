	</request-map>
	<request-map uri="createEmployer">
		<security https="true" auth="true" />
		<event type="service" path="" invoke="createEmployer" />
		<response name="success" type="view" value="employers" />
		<response name="error" type="view" value="EditEmployer" />
	</request-map>

