    <screen name="RegistryReports">
        <section>
            <actions>
            <set field="headerItem" value="Reports" />
            
            </actions>
            <widgets>
                <decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
                    <decorator-section name="body">
                        <container style="lefthalf">
                            <screenlet title="All Activities File Movement Report">
                                <include-form name="fileMovementForm" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        <container style="righthalf">
                         <screenlet title="Single Activity File Movement Report">
                                <include-form name="singleActivityfileMovementForm" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                         <container style="lefthalf">
                            <screenlet title="Returned Files Per User Report">
                                <include-form name="returnedFilesPerUserForm" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        <container style="righthalf">
                         <screenlet title="Returned Files Per Date Report">
                                <include-form name="returnedFilesPerDateForm" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                             </container>
                             <container style="lefthalf">
                            <screenlet title="Duration Between File Request and Issuance Report">
                                <include-form name="durationBeforeIssuanceForm" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                         <container style="righthalf">
                         <screenlet title="Overstayed Files Report">
                                <include-form name="overStayedFiles" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                             </container>
                             
                              <container style="lefthalf">
                            <screenlet title="Report on Inactive Files">
                                <include-form name="inactiveFiles" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        
                         <container style="righthalf">
                         <screenlet title="Report on Semi-Active Files">
                                <include-form name="semiActiveFiles" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                             </container>
                             
                              <container style="lefthalf">
                            <screenlet title="Report on Files in Archive">
                                <include-form name="filesInArchive" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        
                              <container style="righthalf">
                            <screenlet title="Report on Files in Disposal">
                                <include-form name="filesInDisposal" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        
                         <container style="lefthalf">
                            <screenlet title="Report on Deceased Member Files">
                                <include-form name="deceasedMemberFiles" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        
                         <container style="righthalf">
                            <screenlet title="Report on Files Volumes Created">
                                <include-form name="fileVolumesCreatedByDateForm" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        
                         <container style="lefthalf">
                            <screenlet title="Report on Files Requests As Per Activity">
                                <include-form name="fileRequestByActivityForm" location="component://registry/widget/ReportForms.xml"/>
                            </screenlet>
                        </container>
                        
                         <container style="righthalf">
                            <screenlet title="Report on Files In Circulation">
                                <include-form name="filesInCirculationForm" location="component://registry/widget/ReportForms.xml"/>d
                            </screenlet>
                        </container>
                        <container style="clear"/>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>

</screens>
