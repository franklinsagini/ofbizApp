        <actions>
            <entity-condition entity-name="FileType"></entity-condition>
        </actions>
       <field name="fileTypeId"><display /></field>
       <field name="fileType" title="Document Type"> <display/> </field>
       <field name="deleteLink" title="${uiLabelMap.CommonDelete}" widget-style="buttontext">
                <hyperlink target="deleteSupplier" description="${uiLabelMap.CommonDelete}" also-hidden="false">
                    <parameter param-name="userId"/>
                </hyperlink>
        </field>
        <field name="submitButton" title="${uiLabelMap.CommonUpdate}" widget-style="buttontext"> <submit button-type="button"/></field>
    </form>
    
    
    
    
    <form name="EditFileBookForm" type="single" target="createFileBook" focus-field-name="name" header-row-style="header-row" default-table-style="basic-table"
        default-map-name="FileBookContext">
        
        <actions>
            <entity-condition entity-name="FileBooks" list="filebooksList">
                <order-by field-name="fileBookId"/>
            </entity-condition>
            <entity-condition entity-name="UsersHW" list="partydList">
                <order-by field-name="userId"/> 
            </entity-condition>
              <entity-condition entity-name="fileLocations" list="locationList">
                <order-by field-name="locationId"/> 
            </entity-condition>
        </actions>
        <alt-target use-when="FileBookContext!=null" target="updateFileBooks"/>
      
        <field name="fileBookId"><hidden/></field>
        <field name="fileBookId" use-when="FileBookContext!=null" tooltip="${uiLabelMap.CommonNotModifRecreat}"><display /></field>
         <field name="firstName" title="Member ID" widget-style="required" required-field="true">
        <drop-down>
        		<list-options key-name="firstName" list-name="partydList" description="${userId}->(For member-${firstName} ${lastName})"/>
        	</drop-down>
        	</field>
        
        <field name="Status" title="File Status"> 
            <drop-down allow-empty="false">
                <option key="active" description="Active"/>
                <option key="semiactive" description="Semi Active"/>
                <option key="dormant" description="Dormant"/>
            </drop-down>
        </field>
        <field name="filelocation" title="File Location" widget-style="required" required-field="true">
        <drop-down>
        		<list-options key-name="filelocation" list-name="locationList" description="${filelocation}"/>
        	</drop-down>
        	</field>
         
        <field name="CreationDate" title="Creation Date"> <date-time/> </field>
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit button-type="button"/></field>
                <!-- submitButton -->
        <field use-when="FileBookContext==null" name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="buttontext"><submit button-type="text-link"/></field>
        <field use-when="FileBookContext!=null" name="submitButton" title="${uiLabelMap.CommonUpdate}" widget-style="buttontext"><submit button-type="text-link"/></field>
    </form>
    
    
