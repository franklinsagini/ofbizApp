			<hyperlink target="deletefileType" description="${uiLabelMap.CommonDelete}"
				also-hidden="false">
				<parameter param-name="fileTypeId" />
			</hyperlink>
		</field>
		<field name="submitButton" title="${uiLabelMap.CommonUpdate}"
			widget-style="buttontext">
			<submit button-type="button" />
		</field>
	</form>




