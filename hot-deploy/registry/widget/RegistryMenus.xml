<?xml version="1.0" encoding="UTF-8"?>
<menus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-menu.xsd">
	<menu name="MainAppBar" title="${uiLabelMap.RegistryApplication}"
		extends="CommonAppBarMenu" extends-resource="component://common/widget/CommonMenus.xml">

		<menu-item name="main" title="main">
			<link target="main" />
		</menu-item>
		<menu-item name="addlocations" title="File locations">
			<link target="editlocation" />
		</menu-item>
		<menu-item name="addfiletype" title="File Type">
			<link target="editfiletypeURL" />
		</menu-item>
		<menu-item name="fileheader" title="Files Header ">
			<link target="editfilebookURL" />
		</menu-item>
		<menu-item name="addfile" title="Files">
			<link target="editfileURL" />
		</menu-item>
		
	</menu>

	<!-- Files Tab bar Menu -->
	<menu name="FilesTabBar" extends="CommonTabBarMenu"
		extends-resource="component://common/widget/CommonMenus.xml">

		<menu-item name="find" title="${uiLabelMap.CommonFind}">
			<link target="findFile" />
		</menu-item>

		<menu-item name="req" title="Request File">
			<link target="reqfileURL" />
		</menu-item>

		<menu-item name="editFile" title="Edit File">
			<link target="editfilebook">
				<parameter param-name="fileBookId" from-field="FileBookContext.fileBookId" />
			</link>
		</menu-item>

		<menu-item name="adddocs" title="Add Document">
			<link target="editfile">
			</link>
		</menu-item>
	</menu>

	<menu name="FileSubTabBar" menu-container-style="button-bar button-style-2"
		default-selected-style="selected">
		<menu-item name="find" title="Find File" widget-style="buttontext create">
			<link target="main" />
		</menu-item>


	</menu>

	<menu name="FileSubTabBar2" menu-container-style="button-bar button-style-2"
		default-selected-style="selected">
		<menu-item name="fileoverview" title="File Overview"
			widget-style="buttontext create">
			<link target="fileoverviewURL" />
		</menu-item>


	</menu>


	<menu name="FileoverviewTabBar" menu-container-style="button-bar button-style-2"
		default-selected-style="selected">
		<menu-item name="file" title="Create File" widget-style="buttontext create">
			<link target="editfilebookURL" />
		</menu-item>


	</menu>

	<menu name="FileUpdateTabBar" menu-container-style="button-bar button-style-2"
		default-selected-style="selected">
		<menu-item name="file" title="Create File" widget-style="buttontext create">
			<link target="editfilebookURL" />
		</menu-item>
		<menu-item name="fileoverview" title="File Overview"
			widget-style="buttontext create">
			<link target="fileoverviewURL" />
		</menu-item>

	</menu>
</menus>

