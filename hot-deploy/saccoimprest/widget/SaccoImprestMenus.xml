<?xml version="1.0" encoding="UTF-8"?>
<menus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-menu.xsd">

    <menu name="MainAppBar" title="${uiLabelMap.SaccoImprestApplication}" extends="CommonAppBarMenu" extends-resource="component://common/widget/CommonMenus.xml">
        <menu-item name="main" title="${uiLabelMap.CommonMain}">
            <link target="FindImprest"/>
        </menu-item>
        <menu-item name="surrenderableImprest" title="Surrenderable Imprest">
            <link target="FindImprest"/>
        </menu-item>
        <menu-item name="perDiem" title="Per Diem">
            <link target="main"/>
        </menu-item>
        <menu-item name="pettyCash" title="Petty Cash">
            <link target="main"/>
        </menu-item>
        <menu-item name="reports" title="Reports">
            <link target="main"/>
        </menu-item>
    </menu>

    <menu name="ImprestTabBar" extends="CommonTabBarMenu" extends-resource="component://common/widget/CommonMenus.xml">
        <menu-item name="imprestType" title="Find">
            <link target="FindImprest"/>
        </menu-item>
        <menu-item name="imprests" title="Requisition">
            <link target="EditImprest"/>
        </menu-item>
        <menu-item name="imprestsSurrender" title="Surrender">
            <link target="FindSurrender"/>
        </menu-item>
    </menu>

    <menu name="ImprestTypeSubTabBar" menu-container-style="button-bar button-style-2" default-selected-style="selected">
        <menu-item name="createNew" title="${uiLabelMap.CommonCreateNew}" widget-style="buttontext create">
            <link target="CreateNewImprestType"/>
        </menu-item>
    </menu>

    <menu name="ImprestSubTabBar" menu-container-style="button-bar button-style-2" default-selected-style="selected">
        <menu-item name="EditImprest" title="Edit Requisition Header">
            <link target="EditImprest">
                <parameter param-name="imprestId"/>
            </link>
        </menu-item>
        <menu-item name="EditImprestItems" title="Edit Requisition Lines">
            <link target="EditImprestItems">
                <parameter param-name="imprestId"/>
            </link>
        </menu-item>
    </menu>

</menus>
