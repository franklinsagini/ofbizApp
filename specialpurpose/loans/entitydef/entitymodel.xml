<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of an Open For Business Project Component</title>
    <description>None</description>
    <version>1.0</version>
    <default-resource-name>ExampleEntityLabels</default-resource-name>

    <!-- ========================================================= -->
    <!-- ======================== Data Model ===================== -->
    <!-- The modules in this file are as follows:                  -->
    <!--  - org.ofbiz.example.example -->
    <!--  - org.ofbiz.example.feature -->
    <!-- ========================================================= -->

    <!-- ========================================================= -->
    <!-- org.ofbiz.example.example -->
    <!-- ========================================================= -->

    <entity entity-name="Example" package-name="org.ofbiz.example.example" title="Example Entity">
        <field name="exampleId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="exampleTypeId" type="id-ne"></field>
        <field name="statusId" type="id-ne"></field>
        <field name="exampleName" type="name"></field>
        <field name="description" type="description"></field>
        <field name="longDescription" type="very-long"></field>
        <field name="comments" type="comment"></field>
        <field name="exampleSize" type="numeric"></field>
        <field name="exampleDate" type="date-time"></field>
        <field name="anotherDate" type="date-time"></field>
        <field name="anotherText" type="long-varchar"></field>
        <prim-key field="exampleId"/>
        <relation type="one" fk-name="EXMPL_TYP" rel-entity-name="ExampleType">
            <key-map field-name="exampleTypeId"/>
        </relation>
        <relation type="one" fk-name="EXMPL_STTS" rel-entity-name="StatusItem">
            <key-map field-name="statusId"/>
        </relation>
    </entity>
    <entity entity-name="ExampleItem" package-name="org.ofbiz.example.example" title="Example Item Entity">
        <field name="exampleId" type="id-ne"></field>
        <field name="exampleItemSeqId" type="id-ne"><description>secondary sequenced ID</description></field>
        <field name="description" type="description"></field>
        <field name="amount" type="floating-point"></field>
        <field name="amountUomId" type="id"></field>
        <prim-key field="exampleId"/>
        <prim-key field="exampleItemSeqId"/>
        <relation type="one" fk-name="EXMPLIT_UOM" title="Amount" rel-entity-name="Uom">
            <key-map field-name="amountUomId" rel-field-name="uomId"/>
        </relation>
        <relation type="one" fk-name="EXMPLIT_EXMP" rel-entity-name="Example">
            <key-map field-name="exampleId"/>
        </relation>
    </entity>
    <entity entity-name="ExampleStatus" package-name="org.ofbiz.example.example" title="Example Status Entity">
        <field name="exampleId" type="id-ne"></field>
        <field name="statusDate" type="date-time"></field>
        <field name="statusEndDate" type="date-time"></field>
        <field name="statusId" type="id-ne"></field>
        <prim-key field="exampleId"/>
        <prim-key field="statusDate"/>
        <relation type="one" fk-name="EXMPLST_EXMPL" rel-entity-name="Example">
            <key-map field-name="exampleId"/>
        </relation>
        <relation type="one" fk-name="EXMPLST_STTS" rel-entity-name="StatusItem">
            <key-map field-name="statusId"/>
        </relation>
    </entity>
    <entity entity-name="ExampleType"
            package-name="org.ofbiz.example.example"
            title="Example Type Entity">
        <field name="exampleTypeId" type="id-ne"></field>
        <field name="parentTypeId" type="id"></field>
        <field name="description" type="description"></field>
        <prim-key field="exampleTypeId"/>
        <relation type="one" fk-name="EXMPLTP_PAR" rel-entity-name="ExampleType">
            <key-map field-name="parentTypeId" rel-field-name="exampleTypeId"/>
        </relation>
    </entity>

    <!-- ========================================================= -->
    <!-- org.ofbiz.example.feature -->
    <!-- ========================================================= -->

    <entity entity-name="ExampleFeature"
        package-name="org.ofbiz.example.feature"
        title="Example Feature Entity">
        <field name="exampleFeatureId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="featureSourceEnumId" type="id-ne"></field>
        <field name="description" type="description"></field>
        <prim-key field="exampleFeatureId"/>
        <relation type="one" fk-name="EXFT_ENUM" rel-entity-name="Enumeration">
            <key-map field-name="featureSourceEnumId" rel-field-name="enumId"/>
        </relation>
    </entity>
    <entity entity-name="ExampleFeatureAppl"
        package-name="org.ofbiz.example.feature"
        title="Example Feature Application Entity">
        <field name="exampleId" type="id-ne"></field>
        <field name="exampleFeatureId" type="id-ne"></field>
        <field name="fromDate" type="date-time"></field>
        <field name="thruDate" type="date-time"></field>
        <field name="exampleFeatureApplTypeId" type="id-ne"></field>
        <field name="sequenceNum" type="numeric"></field>
        <prim-key field="exampleId"/>
        <prim-key field="exampleFeatureId"/>
        <prim-key field="fromDate"/>
        <relation type="one" fk-name="EXFTAP_EXPL" rel-entity-name="Example">
            <key-map field-name="exampleId"/>
        </relation>
        <relation type="one" fk-name="EXFTAP_EXFT" rel-entity-name="ExampleFeature">
            <key-map field-name="exampleFeatureId"/>
        </relation>
        <relation type="one" fk-name="EXFTAP_TYP" rel-entity-name="ExampleFeatureApplType">
            <key-map field-name="exampleFeatureApplTypeId"/>
        </relation>
    </entity>
    <entity entity-name="ExampleFeatureApplType"
        package-name="org.ofbiz.example.feature"
        title="Example Feature Application Type Entity">
        <field name="exampleFeatureApplTypeId" type="id-ne"></field>
        <field name="parentTypeId" type="id"></field>
        <field name="description" type="description"></field>
        <prim-key field="exampleFeatureApplTypeId"/>
        <relation type="one" fk-name="EXFTAPTP_PAR" rel-entity-name="ExampleFeatureApplType">
            <key-map field-name="parentTypeId" rel-field-name="exampleFeatureApplTypeId"/>
        </relation>
    </entity>
    
    <!-- Sacco Product and Applications -->
    <!-- -org.ofbiz.loans -->
	<entity entity-name="SaccoProduct" package-name="org.ofbiz.loans"
		title="Sacco Product Entity">
		<field name="saccoProductId" type="id-ne"></field>
		<field name="name" type="name"></field>
		<field name="code" type="name"></field>
		<field name="minBalanceAmt" type="fixed-point"></field>
		<field name="minSavingsAmt" type="fixed-point"></field>
		<field name="minInterestEarningbalAmt" type="fixed-point"></field>
		<field name="productType" type="name"></field>
		<field name="perYearWithdrawals" type="name"></field>
		<field name="noofWithdrawals" type="numeric"></field>
		<field name="minNoofPeople" type="numeric"></field>
		<field name="maxAmt" type="fixed-point"></field>
		<field name="percentInterestPerMonthAmt" type="fixed-point"></field>

		<field name="maxRepaymentPeriod" type="numeric"></field>
		<field name="maxMultipleofSavings" type="name"></field>
		<field name="multipleCount" type="numeric"></field>
		<field name="maxPercentageofNetSalary" type="numeric"></field>

		<!-- Extra Account -->
		<field name="allowPercentageDiscountonBankCheque" type="name"></field>
		<field name="percentageDiscountonBankChequeAmt" type="fixed-point"></field>
		<field name="minimumPeriodMonths" type="numeric"></field>
		<field name="allowLedgerFees" type="name"></field>

		<!-- Extra Loan -->
		<field name="retainBosaDeposit" type="name"></field>
		<field name="retainBosaDepositAmt" type="numeric"></field>
		<field name="maxIsPercentageOfMemberSalary" type="name"></field>
		<field name="percentageOfMemberSalaryAmt" type="numeric"></field>
		<field name="salaryPointShouldbeFosa" type="name"></field>

		<field name="paymentsProcessDeductsfromMonthly" type="name"></field>
		<field name="paymentsProcessDeductsfromBonus" type="name"></field>
		
		<field name="loanStatus" type="name"></field>
		
		<field name="minimumPeriodToEligibility" type="numeric"></field>

		<prim-key field="saccoProductId" />

	</entity>
	
	<!-- org.ofbiz.loans.LoanSecurity -->
	<entity entity-name="LoanSecurity" package-name="org.ofbiz.loans"
		title="Loan Security Entity">
		<field name="loanSecurityId" type="id-ne"></field>
		<field name="description" type="description"></field>
		<prim-key field="loanSecurityId"/>
	</entity>
	
	<!-- org.ofbiz.loans.Bank -->
	<entity entity-name="Bank" package-name="org.ofbiz.loans.Bank"
		title="Bank Entity">
		<!-- field name="loanSecurityId" type="id-ne"></field -->
		<field name="bankId" type="id-ne"></field>
		<field name="description" type="description"></field>
		
		<prim-key field="bankId"/>
		
	</entity>

	<!-- -org.ofbiz.loans -->
	<entity entity-name="LoanApplication" package-name="org.ofbiz.loans"
		title="Loan Application Entity" sequence-bank-size="1">
		<field name="loanApplicationId" type="id-ne"></field>
		
		<field name="loanNo" type="name"></field>
		<field name="loanNoSeq" type="name"></field>
		<field name="partyId" type="id-ne"></field>
	  
		<field name="firstName" type="name"></field>
		<field name="middleName" type="name"></field>
		<field name="lastName" type="name"></field>
		
		<field name="idNumber" type="name"></field>
		<field name="memberType" type="very-short"></field>
		<field name="memberNumber" type="name"></field>
		<field name="mobileNumber" type="name"></field>
		
		
		
		<!-- Loan Type -->
		<field name="saccoProductId" type="id-ne"></field>
		
		<field name="percentInterestPerMonthAmt" type="fixed-point"></field>

		<field name="maxRepaymentPeriod" type="numeric"></field>
		
		<field name="loanamt" type="fixed-point"></field>
		
		<field name="selectedRepaymentPeriod" type="numeric"></field>
		
		<field name="paymentMethodTypeId" type="id-ne"></field>
				
		<field name="loanStatusId" type="id"></field>
		
		<!-- Extra Loan Fields -->
		<field name="loanDuration" type="numeric"></field>
		<field name="loanStartDate" type="date-time"></field>
		<field name="outstandingBalance" type="fixed-point"></field>
		<field name="principalDue" type="fixed-point"></field>
		<field name="interestDue" type="fixed-point"></field>
		<field name="panaltyInterestDue" type="fixed-point"></field>
		<field name="monthlyRepayment" type="fixed-point"></field>
		<field name="nextInstallmentDate" type="date-time"></field>
		
		<field name="isOpeningBalance" type="name"></field>
		
		<field name="applicationStatus" type="name"></field>
		
		<prim-key field="loanApplicationId"/>
		<relation type="one" fk-name="PAYMENT_PMETH_TP" rel-entity-name="PaymentMethodType">
        <key-map field-name="paymentMethodTypeId"/>
      </relation>
      <relation type="one" fk-name="SACCO_PRODUCT_FK" rel-entity-name="SaccoProduct">
        <key-map field-name="saccoProductId"/>
      </relation>
      <relation type="one" fk-name="MEMBER_FK" rel-entity-name="Member">
        <key-map field-name="partyId"/>
      </relation>
      
      <relation type="one" fk-name="LOANSTATUS_FK" rel-entity-name="LoanStatus">
        <key-map field-name="loanStatusId"/>
      </relation>
	</entity>
	
	<!-- Loan Application Security -->
	<entity entity-name="LoanApplicationSecurity" package-name="org.ofbiz.loans"
		title="Loan Application Security Entity">
		<field name="loanApplicationSecurityId" type="id-ne"></field>
		<field name="loanApplicationId" type="id"></field>
		<field name="loanSecurityId" type="id"></field>
		
		<prim-key field="loanApplicationSecurityId"/>
		<relation type="one" fk-name="LOANSECURITY_FK" rel-entity-name="LoanSecurity">
        	<key-map field-name="loanSecurityId"/>
      	</relation>
      	<relation type="one" fk-name="LOANAPPLICATION_FK" rel-entity-name="LoanApplication">
       		<key-map field-name="loanApplicationId"/>
      	</relation>
	</entity>
	
	<!-- Loan Application Collateral -->
	<entity entity-name="LoanApplicationCallateral" package-name="org.ofbiz.loans"
		title="Loan Application Collateral Entity">
		<field name="loanApplicationCallateralId" type="id-ne"></field>
		<field name="loanApplicationId" type="id"></field>
		<field name="collateralValue" type="fixed-point"></field>
		<field name="valuingAgency" type="name"></field>
		<field name="collateralType" type="name"></field>
		<field name="collateralSerialNo" type="name"></field>
		<field name="collateralOwner" type="name"></field>
		<field name="collateralStatus" type="name"></field>
		<field name="propotionofloanamt" type="fixed-point"></field>
		<prim-key field="loanApplicationCallateralId"/>
		<relation type="one" fk-name="COLLATERAL_LOANAPPLICATION_FK" rel-entity-name="LoanApplication">
       		<key-map field-name="loanApplicationId"/>
      	</relation>
	</entity>
	
	<!-- Collateral Document -->
	<entity entity-name="CollateralDocument" package-name="org.ofbiz.loans"
		title="Collateral Document Entity">
		<field name="collateralDocumentId" type="id-ne"></field>
		<field name="loanApplicationCallateralId" type="id"></field>
		<prim-key field="collateralDocumentId"/>
		<relation type="one" fk-name="COLLATERAL_FK" rel-entity-name="LoanApplicationCallateral">
       		<key-map field-name="loanApplicationCallateralId"/>
      	</relation>
	</entity>
	
	<!-- Loan Guarantors -->
	<entity entity-name="MemberStation" package-name="org.ofbiz.loans"
		title="Member Station Entity">
		<field name="memberStationId" type="id-ne"></field>
		<field name="name" type="name"></field>
		<prim-key field="memberStationId"/>
	</entity>
	
	<!-- Loan Guarantors -->
	<entity entity-name="LoanGuarantor" package-name="org.ofbiz.loans"
		title="Loan Guarantor Entity">
		<field name="loanGuarantorId" type="id-ne"></field>
		<field name="loanApplicationId" type="id"></field>
		<field name="guarantorId" type="id"></field>
		<field name="memberNo" type="name"></field>
		<field name="payrolNo" type="name"></field>
		<field name="depositamt" type="fixed-point"></field>
		<field name="currentStationId" type="id"></field>
		<prim-key field="loanGuarantorId"/>
		<relation type="one" fk-name="GUARANTOR_LOANAPPLICATION_FK" rel-entity-name="LoanApplication">
       		<key-map field-name="loanApplicationId"/>
      	</relation>
      	<relation type="one" fk-name="GUARANTOR_MEMBER_FK" rel-entity-name="Member">
       		<key-map field-name="guarantorId" rel-field-name="partyId" />
      	</relation>
      	<relation type="one" fk-name="GUARANTOR_MEMBERSTATION_FK" rel-entity-name="MemberStation">
       		<key-map field-name="currentStationId" rel-field-name="memberStationId" />
      	</relation>
	</entity>
	
	<!-- Loan Status -->
	<entity entity-name="LoanStatus" package-name="org.ofbiz.loans"
		title="Loan Status Entity">
		<field name="loanStatusId" type="id-ne"></field>
		<field name="name" type="name"></field>
		<prim-key field="loanStatusId"/>
	</entity>
	
	<entity entity-name="MemberAccountDetails" package-name="org.ofbiz.loans"
		title="MemberAccountDetails Entity">
		<field name="memberAccountDetailsId" type="id-ne"></field>
		<field name="partyId" type="id"></field>
		<field name="openingBalanceDate" type="date-time"></field>
		<!-- field name="memberAccountPayableId" type="id"></field -->
		<field name="glAccountId" type="id"></field>
		
		<field name="apOpeningBalance" type="fixed-point"></field>
		<field name="savingsOpeningBalance" type="fixed-point"></field>
		<prim-key field="memberAccountDetailsId"/>
		<relation type="one" fk-name="MEMBERACCDETAILS_ACC" title="Parent" rel-entity-name="GlAccount">
        	<key-map field-name="memberAccountPayableId" rel-field-name="glAccountId"/>
      </relation>
      
       <relation type="one" fk-name="ACCOUNTDETAILS_MEMBER" rel-entity-name="Member">
        <key-map field-name="partyId"/>
      </relation>
	</entity>
	
	<entity entity-name="MemberShare" package-name="org.ofbiz.loans"
		title="MemberShare Entity">
		<field name="memberShareId" type="id-ne"></field>
		<field name="partyId" type="id"></field>
		<field name="shareAccountId" type="id"></field>
		<field name="sharePrice" type="fixed-point"></field>
		<field name="noOfShares" type="numeric"></field>
		<field name="openingBalance" type="fixed-point"></field>
		<prim-key field="memberShareId"/>
		<relation type="one" fk-name="MEMBERSHARE_ACC" title="Parent" rel-entity-name="GlAccount">
        <key-map field-name="shareAccountId" rel-field-name="glAccountId"/>
      </relation>
      
       <relation type="one" fk-name="MEMBERSHARE_MEMBER" rel-entity-name="Member">
        <key-map field-name="partyId"/>
      </relation>
	</entity>
	
	<entity entity-name="LoanStatusLog" package-name="org.ofbiz.loans"
		title="LoanStatusLog Entity">
		<field name="loanStatusLogId" type="id-ne"></field>
		<field name="loanApplicationId" type="id"></field>
		
		<field name="isActive" type="indicator"></field>
		<field name="createdBy" type="name"></field>
		<field name="updatedBy" type="name"></field>
		
		<field name="applicationStatus" type="name"></field>
		<field name="comment" type="name"></field>
		
		<field name="branchId" type="name"></field>
		
		<prim-key field="loanStatusLogId"/>
		<!-- relation type="one" fk-name="LoanStatusLog_MemberCreate" title="Parent" rel-entity-name="Member">
        <key-map field-name="createdBy" rel-field-name="partyId"/>
      </relation>
      
      <relation type="one" fk-name="LoanStatusLog_MemberUpdate" title="Parent" rel-entity-name="Member">
        <key-map field-name="updatedBy" rel-field-name="partyId"/>
      </relation -->
      
       <relation type="one" fk-name="LoanStatusLog_LoanApp" rel-entity-name="LoanApplication">
        <key-map field-name="loanApplicationId"/>
      </relation>
      
      <relation type="one" fk-name="LoanStatusLog_branch" rel-entity-name="PartyGroup">
        <key-map field-name="branchId" rel-field-name="partyId"/>
      </relation>
	</entity>
	
	
	
</entitymodel>
