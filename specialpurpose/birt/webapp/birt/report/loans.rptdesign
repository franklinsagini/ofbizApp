            <xml-property name="queryText"><![CDATA[select loan_no, coalesce(first_name, '')+' '+coalesce(last_name, '') as member_names, application_status, member_number, id_number, loan_amt, repayment_period, app.interest_rate_p_m, cast(disbursement_date as date) disbursement_date,
 cast(repayment_start_date as date) repayment_start_date,
  prod.code+'-'+prod.name as loan_type from LOAN_APPLICATION app
   JOIN LOAN_PRODUCT prod ON (app.LOAN_PRODUCT_ID=prod.LOAN_PRODUCT_ID) ]]></xml-property>
