            <xml-property name="queryText"><![CDATA[select 
party_skill.party_id,
party_skill.skill_type_id,
skill_type.description as skill,
Person.first_name,
Person.last_name,
Person.employee_number as payroll_No,
Person.party_id as staff_party_id
from 
party_skill
INNER JOIN skill_type ON skill_type.skill_type_id 
    = party_skill.skill_type_id
INNER JOIN Person ON Person.party_id 
    = party_skill.party_id

WHERE
Person.employee_number = ?
    ]]></xml-property>
