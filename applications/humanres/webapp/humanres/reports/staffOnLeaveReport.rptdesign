            <xml-property name="queryText"><![CDATA[SELECT case empl_leave.leave_type_id when 'ANNUAL_LEAVE' then 'Annual Leave'
    when 'INLAND_EARNED' then 'Inland Earned'
    ELSE '' end as leave_type_id, 
    empl_leave.empl_leave_reason_type_id, 
    empl_leave.party_id, 
    empl_leave.from_date,
    empl_leave.leave_duration,
    empl_leave.thru_date,
    empl_leave.leave_status,
    Person.first_name,
    Person.last_name 

  FROM empl_leave INNER JOIN Person ON empl_leave.party_id 
    = Person.party_id
 ]]></xml-property>
