            <xml-property name="queryText"><![CDATA[SELECT  REPLACE(empl_leave.leave_type_id, '_', ' '),
    empl_leave.empl_leave_reason_type_id, 
    empl_leave.party_id, 
    empl_leave.from_date,
    empl_leave.leave_duration,
    empl_leave.thru_date,
    empl_leave.approval_status,
    Person.first_name,
    Person.last_name,
    
    empl_leave.thru_date + interval '1 days' as resumption_date
    
    

   FROM empl_leave INNER JOIN Person ON empl_leave.party_id 
    = Person.party_id]]></xml-property>
