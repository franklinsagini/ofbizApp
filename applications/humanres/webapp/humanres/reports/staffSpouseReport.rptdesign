            <xml-property name="queryText"><![CDATA[select 
staff_spouse.family_relations_id,
staff_spouse.party_id,
staff_spouse.fullname,
staff_spouse.gender,
staff_spouse.dob,
family_relations.relationship,
Person.first_name,
Person.last_name,
Person.employee_number as payroll_No,
Person.party_id as staff_party_id
from 
staff_spouse
INNER JOIN family_relations ON family_relations.family_relations_id 
    = staff_spouse.family_relations_id
INNER JOIN Person ON Person.party_id 
    = staff_spouse.party_id

WHERE
Person.employee_number = ?
    
]]></xml-property>
