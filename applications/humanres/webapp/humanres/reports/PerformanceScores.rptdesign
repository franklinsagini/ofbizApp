            <xml-property name="queryText"><![CDATA[select p.employee_number, p.first_name||' '||p.last_name as employee, 
p2.employee_number as apparaiser_number, p2.first_name||' '||p2.last_name as appraiser, kpitarget,
prt.description as rating, prt.points as ratingscore, kas.achievementstatus, pri.achievement_date,
(select max(points) from perf_rating_type) as total_points, pr.from_date, pr.thru_date
 from perf_review_item  pri 
left join perf_review pr on pr.perf_review_id=pri.perf_review_id 
left join person p on p.party_id=pr.employee_party_id 
left join person p2 on p2.party_id=pr.manager_party_id 
left join perf_rating_type prt on prt.perf_rating_type_id=pri.perf_rating_type_id 
left join k_p_i_achievement_status kas on kas.kpi_achievement_status_id=pri.kpi_achievement_status_id
left join k_p_i_targets kpt on kpt.kpi_target_id=pri.kpi_target_id
where p.party_id='Developer2' and pr.status='Submitted' 
Group by p.employee_number, p.first_name,p.last_name,p2.employee_number,
p2.first_name,p2.last_name, kpitarget,
prt.description, prt.points , kas.achievementstatus, pri.achievement_date, pr.from_date, pr.thru_date]]></xml-property>
