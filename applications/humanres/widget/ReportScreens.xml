                				<container style="button-bar">
                				<link target="stafflistcsv" text="Staff List CSV" style="buttontext">
                                                <image url-mode="raw" src="${iconsLocation}/page_excel.png"/>
                                            </link>
                                <link target="stafflistpdf" text="Staff List PDF" style="buttontext">
                                                <image url-mode="raw" src="${iconsLocation}/page_white_acrobat.png"/>
                                            </link>
                				</container>
                				</screenlet>
                        		
                        	</widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
                		</screenlet>
                		
                		
                	</widgets>
                </section>
            </widgets>
        </section>
    </screen>
