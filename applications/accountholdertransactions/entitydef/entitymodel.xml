        </relation>
    </entity>
    
     <entity entity-name="StandingOrder" package-name="org.ofbiz.accountholdertransactions" title="StandingOrder Entity">
        <field name="standingOrderId" type="id-vlong-int"><description>primary sequenced ID</description></field>
        <field name="isActive" type="indicator"></field>
        <field name="createdBy" type="id"></field>
        <field name="updatedBy" type="id"></field>
        <field name="partyId" type="id-vlong-int"></field>
        <field name="branchId" type="id-int"></field>
        
        <field name="memberNo" type="name"></field>
        <field name="payrollNo" type="name"></field>
     
     	<field name="standingOrderAmount" type="currency-amount"></field>
     	<field name="orderStartDate" type="date-time"></field>
     	
     	<field name="internalexternal" type="name"></field> <!-- INTERNAL EXTERNAL -->
     	<field name="transactionTYpe" type="name"></field> <!-- LOAN OTHERS -->
     	
     	<!-- LOAN -->
     	
         <field name="loanTypeId" type="id-int"></field> <!-- Sacco Product Id type of LOAN -->
