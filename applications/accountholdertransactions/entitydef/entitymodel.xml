         <field name="reference" type="id"   ><description>Reference ID</description></field>

         <field name="systemtrace" type="id"   ><description>Reference ID</description></field>

         <field name="reverseStatus" type="id"   ><description>Reverse Status</description></field>

         <field name="monthremitted" type="id"   ></field>
