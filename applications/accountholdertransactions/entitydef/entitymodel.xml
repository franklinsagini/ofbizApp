        <relation type="one" fk-name="TreasuryTransactionDest_Treasury" rel-entity-name="Treasury">
            <key-map  field-name="treasuryId"/>
        </relation>

        <relation type="one" fk-name="AccountTransaction_Charge" rel-entity-name="ProductCharge">
