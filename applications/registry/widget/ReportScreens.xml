	 <screen name="fileMovementCSV">
        <section>
            <actions>
                <property-map resource="ProductUiLabels" map-name="uiLabelMap" global="true"/>
                <property-map resource="CommonUiLabels" map-name="uiLabelMap" global="true"/>

                <set field="facilityId" from-field="parameters.facilityId"/>
                <set field="viewIndex" from-field="parameters.VIEW_INDEX" type="Integer" default-value="0"/>
                <property-to-field resource="widget" property="widget.form.defaultViewSize" field="viewSizeDefaultValue"/>
                <set field="viewSize" from-field="parameters.VIEW_SIZE" type="Integer" default-value="${viewSizeDefaultValue}"/>
                
                <entity-one value-field="member" entity-name="Member">
			   <field-map field-name="partyId" value="${parameters.partyId}"/>
		       </entity-one>

                <!-- <entity-one entity-name="Facility" value-field="facility" use-cache="true"/> -->
                <script location="component://registry/webapp/registry/reports/fileMovementStatementExport.groovy"/>
            </actions>
            <widgets>
                <container>
                    <label style="h1" text="MEMBER FILE MOVEMENT"/>
                    <label style="h3" text="Payroll Number: ${member.payrollNumber}"/>
                    <label style="h3" text="Member Name: ${member.firstName} ${member.lastName}"/>
                </container>
                <include-form name="fileMovementExport" location="component://registry/widget/ReportForms.xml"/>
            </widgets>
        </section>
    </screen>
	
