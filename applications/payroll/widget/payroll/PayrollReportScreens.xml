    <screen name="PayslipGeneratorpdf">
        <section>
            <actions>
                <set field="titleProperty" value="Payslip View"/>                
                <set field="payrollPeriodId" from-field="parameters.payrollPeriodId"/>
                <set field="partyId" from-field="parameters.partyId"/>
                <entity-condition entity-name="PayslipView" list="payslipViewList">
                    <condition-list combine="and">
                        <condition-expr field-name="partyId" operator="equals" from-field="parameters.partyId" ignore-if-empty="true"/>
                        <condition-expr field-name="payrollPeriodId" operator="equals" from-field="parameters.payrollPeriodId" ignore-if-empty="true"/>
                    </condition-list>
                    <!-- note that we need to explicitly identify the fields to select, otherwise the query will include fields that can't logically be grouped -->
                	<select-field field-name="partyId"/>
                
        
                
                </entity-condition>
            </actions>
            <widgets>
                <platform-specific>
                    <xsl-fo><html-template location="component://payroll/webapp/payroll/reports/payslip.fo.ftl"/></xsl-fo>
                </platform-specific>
            </widgets>
        </section>
    </screen>
    
