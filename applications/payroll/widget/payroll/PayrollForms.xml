        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">

    
    
    <!-- New Payroll Element -->
    <form name="NewPayrollElement" type="single" target="createPayrollElement"
        focus-field-name="name" header-row-style="header-row" default-table-style="basic-table" default-map-name="payrollElementContext" paginate-target="elementsDefinition">
         <actions>
        	<entity-condition entity-name="PayrollElement" list="elementList"></entity-condition>
        	<set field="parentId" from-field="payrollElementId"/>
        </actions>	        
        
        <field name="payrollElementId"><hidden/></field>
        <field name="elementCode" title="Code" tooltip="${uiLabelMap.CommonRequired}" widget-style="required"><text size="30" maxlength="60"/></field>
        <field name="name" title="Name" tooltip="${uiLabelMap.CommonRequired}" widget-style="required"><text size="30" maxlength="60"/></field>
        <field name="elementType"  title="Element Type" >
            <drop-down allow-empty="true">
                <option key="Payment" description="Payment"/>
                <option key="Deduction" description="Deduction"/>
                <option key="System Element" description="System Element"/>
            </drop-down>
        </field>
        <field name="iscalulated" title="Calculated" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>                
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        <field name="hasBalance" title="Has Balance" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>                
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        <field name="relief"  title="Is Relief" >
            <drop-down allow-empty="false">                
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
        <field name="cummulative"  title="Is Cummulative" >
            <drop-down allow-empty="false">
            	<option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        <field name="employercontribution"  title="Employer Contribution" >
            <drop-down allow-empty="false">
            	<option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
        <!--field name="systemcontrolled"  title="Control Type" >
            <drop-down allow-empty="false">
            	<option key="User" description="User Controlled "/>
                <option key="System" description="System Controlled"/>
            </drop-down>
        </field-->
        
         <field name="taxable"  title="Is Taxable" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
        <field name="employertax"  title="Employer Tax" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        <field name="benefit"  title="Benefit Type" >
            <drop-down allow-empty="true">
                <option key="Cash" description="Cash Benefit"/>
                <option key="Non-cash" description="Non-cash Benefit"/>
            </drop-down>
        </field>
        <field name="insurancecontribution"  title="Insurance Contribution" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        <field name="hasRelief"  title="Has Relief" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
		<field name="recurrent"  title="Is Recurrent" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
         <field name="isInterest"  title="Is Interest" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
        <field name="hasInterest"  title="Has Interest" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
        <field name="isfixed"  title="Fixed" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        <field name="interestamount" title="Interest Amount"><text size="30" maxlength="60"/></field>
        <field name="interestrate" title="Interest Rate"><text size="30" maxlength="60"/></field>
        <field name="interestfrequency"  title="Interest Frequency" >
            <drop-down allow-empty="false">
                <option key="pa" description="Per Annum"/>
                <option key="pm" description="Per Month"/>
            </drop-down>
        </field>
        <field name="payslipdisplay"  title="Payslip Display" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        <field name="isbold"  title="Bolded" >
            <drop-down allow-empty="false">
                <option key="N" description="No"/>
                <option key="Y" description="Yes"/>
            </drop-down>
        </field>
        
        
        <!-- field name="parentId" title="Parent Element">
        	<drop-down allow-empty="true">
        		<list-options list-name="elementList" key-name="payrollElementId" description="${name}"/>
        	</drop-down>
        </field-->
        
        <field name="childId" title="Child Element">
        	<drop-down allow-empty="true">
        		<list-options list-name="elementList" key-name="payrollElementId" description="${name}"/>
        	</drop-down>
        </field>
        
        
        <field name="submitButton" title="${uiLabelMap.CommonSave}" widget-style="smallSubmit"><submit button-type="text-link"/></field>
    </form>
    
    <form name="listPayrollElements" type="list" list-name="listIt" use-row-submit="true" separate-columns="true" target="updatePayrollElement" paginate-target="elementsDefinition"
        odd-row-style="alternate-row" header-row-style="header-row-2" default-table-style="basic-table hover-bar">
        <actions>
            <entity-condition entity-name="PayrollElement">
            </entity-condition>
            <entity-condition entity-name="PayrollElement" list="payrollElementList"></entity-condition>
        </actions>
        <auto-fields-service default-field-type="hidden" service-name="updatePayrollElement"/>
        <!-- field name="skillTypeId">
            <display-entity entity-name="SkillType" description="${description}"/>
        </field -->
        <field name="name"> <display also-hidden="false"/> </field>
        <field name="elementType"> <display also-hidden="false"/> </field>
        <field name="payslipdisplay"> <display also-hidden="false"/> </field>
        <field name="hasInterest" title="Has Interest"> <display also-hidden="false"/> </field>
        <field name="isfixed" title="Fixed"> <display also-hidden="false"/> </field>
        <field name="interestamount"> <display also-hidden="false"/> </field>
        <field name="interestrate"> <display also-hidden="false"/> </field>
        <field name="interestfrequency"> <display also-hidden="false"/> </field>
        <field name="childId" title="Child Element">
        	<display-entity entity-name="PayrollElement" key-field-name="payrollElementId" description="${name}"></display-entity>
        </field>
        <field name="deleteLink" title="${uiLabelMap.CommonDelete}" widget-style="buttontext">
            <hyperlink target="deletePayrollElement" description="${uiLabelMap.CommonDelete}" also-hidden="false">
                <parameter param-name="payrollElementId"/>
            </hyperlink>
        </field>
        <field name="submitButton" title="${uiLabelMap.CommonUpdate}"><submit button-type="button"/></field>
    </form>
    
