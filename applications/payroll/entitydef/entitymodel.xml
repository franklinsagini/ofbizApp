<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of an Open For Business Project Component</title>
    <description>None</description>
    <version>1.0</version>
    <default-resource-name>PayrollEntityLabels</default-resource-name>

    <!-- ========================================================= -->
    <!-- ======================== Data Model ===================== -->
    <!-- The modules in this file are as follows:                  -->
    <!--  - org.ofbiz.example.example -->
    <!--  - org.ofbiz.example.feature -->
    <!-- ========================================================= -->

    <!-- ========================================================= -->
    <!-- org.ofbiz.example.example -->
    <!-- ========================================================= -->

    <entity entity-name="PayrollElement" package-name="org.ofbiz.payroll" title="Payroll Element Entity">
        <field name="payrollElementId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="name" type="name"></field>
        <field name="elementType" type="name"></field>
        <field name="hasBalance" type="indicator"></field>
        <field name="iscalulated" type="indicator"></field>        
        <field name="relief" type="indicator"></field>
        <field name="cummulative" type="indicator"></field>
        <field name="employercontribution" type="indicator"></field>
        <!--field name="systemcontrolled" type="name"></field-->
        <field name="taxable" type="indicator"></field>
        <field name="employertax" type="indicator"></field>
        <field name="benefit" type="name"></field> <!-- Cash Benefit or Non Cash Benefit -->
        <field name="insurancecontribution" type="indicator"></field>
        <field name="hasRelief" type="indicator"></field>
        <field name="recurrent" type="indicator"></field>
        <field name="hasInterest" type="indicator"></field>
        <field name="isInterest" type="indicator"></field>
        <field name="isfixed" type="indicator"></field>
        <field name="interestamount" type="currency-amount"></field>
        <field name="interestrate" type="floating-point"></field>
        <field name="interestfrequency" type="name"></field>
        <field name="payslipdisplay" type="indicator"></field>
        <field name="isbold" type="indicator"></field>
        <field name="parentId" type="id"></field>
        <field name="childId" type="id"></field>
        
        
        <prim-key field="payrollElementId"/>
        <relation type="one" fk-name="parent_Element" rel-entity-name="PayrollElement">
            <key-map field-name="parentId"/>
        </relation>
        <relation type="one" fk-name="child_Element" rel-entity-name="PayrollElement">
            <key-map field-name="childId"/>
        </relation>
        <!-- relation type="one" fk-name="EXMPL_TYP" rel-entity-name="ExampleType">
            <key-map field-name="exampleTypeId"/>
        </relation>
        <relation type="one" fk-name="EXMPL_STTS" rel-entity-name="StatusItem">
            <key-map field-name="statusId"/>
        </relation -->
    </entity>
    
     <entity entity-name="JobGrade" package-name="org.ofbiz.payroll" title="Job Grade Entity">
        <field name="jobGradeId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="name" type="name"></field>
        <prim-key field="jobGradeId"/>
        </entity>
    
     <entity entity-name="SalaryScale" package-name="org.ofbiz.payroll" title="Salary Scale Entity">
        <field name="salaryScaleId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="jobGradeId" type="id"></field>
        <field name="minAmount" type="currency-amount"></field>
        <field name="maxAmount" type="currency-amount"></field>
        <prim-key field="salaryScaleId"/>
        <relation type="one" fk-name="salary_Jobgrade" rel-entity-name="JobGrade">
            <key-map field-name="jobGradeId"/>
        </relation>
        
        </entity>
        
         <entity entity-name="BankDetails" package-name="org.ofbiz.payroll" title="Bank Details Entity">
        <field name="bankDetailsId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="bankName" type="name"></field>
        <prim-key field="bankDetailsId"/>
        
    	</entity>
    	
    	<entity entity-name="BankBranch" package-name="org.ofbiz.payroll" title="Bank Branch Entity">
        <field name="bankBranchId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="bankDetailsId" type="id"></field>
        <field name="branchName" type="name"></field>
        <field name="swiftCode" type="name"></field>
        <field name="branchNo" type="name"></field>
        <field name="location" type="name"></field>
        <prim-key field="bankBranchId"/>
        <relation type="one" fk-name="branch_bank" rel-entity-name="BankDetails">
            <key-map field-name="bankDetailsId"/>
        </relation>
        </entity>
        
        <entity entity-name="PayPoint" package-name="org.ofbiz.payroll" title="Staff Paypoints Entity">
        <field name="payPointId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="partyId" type="id"></field>
        <field name="staffNo" type="name"></field>
        <field name="staffName" type="name"></field>        
        <field name="bankDetailsId" type="id"></field>
        <field name="bankBranchId" type="id"></field>       
        <field name="accountName" type="name"></field>
        <field name="accountNumber" type="name"></field>
        <field name="payMode" type="name"></field> <!-- EFT or Cheque -->
        <prim-key field="payPointId"/>
        <relation type="one" fk-name="paypoint_bank" rel-entity-name="BankDetails">
            <key-map field-name="bankDetailsId"/>
        </relation>
        <relation type="one" fk-name="paypoint_branch" rel-entity-name="BankBranch">
            <key-map field-name="bankBranchId"/>
        </relation>
        <relation type="one" fk-name="paypoint_party" rel-entity-name="Party">
            <key-map field-name="partyId"/>
        </relation>
        </entity>        
        
        <entity entity-name="PayrollYear" package-name="org.ofbiz.payroll" title="Payroll Year Entity">
        <field name="payrollYearId" type="id-ne"><description>primary sequenced ID</description></field>
       <field name="name" type="name"></field>
       <field name="currentyear" type="indicator"></field>
        <field name="startDate" type="date-time"></field>
        <field name="endDate" type="date-time"></field>
        <prim-key field="payrollYearId"/>
        </entity>
        
        <entity entity-name="PayrollPeriod" package-name="org.ofbiz.payroll" title="Payroll Period Entity">
        <field name="payrollPeriodId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="payrollYearId" type="id"></field>
       <field name="sequence_no" type="numeric"></field>
       <field name="name" type="name"></field>
       <field name="currentperiod" type="indicator"></field>
        <field name="startDate" type="date-time"></field>
        <field name="endDate" type="date-time"></field>
        <prim-key field="payrollPeriodId"/>
        <relation type="one" fk-name="period_year" rel-entity-name="PayrollYear">
            <key-map field-name="payrollYearId"/>
        </relation>
        </entity>
        
        <entity entity-name="StaffPayroll" package-name="org.ofbiz.payroll" title="Staff Payroll Management Entity">
        <field name="staffPayrollId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="payrollPeriodId" type="id"></field>
        <field name="partyId" type="id"></field>
        <field name="staffNo" type="name"></field>
        <field name="staffName" type="name"></field>
       <field name="closed" type="indicator"></field>
       <field name="pensionpercentage" type="currency-amount"></field> 
       <field name="nssfVolAmount" type="currency-amount"></field> 
        <prim-key field="staffPayrollId"/>
        <relation type="one" fk-name="management_period" rel-entity-name="PayrollPeriod">
            <key-map field-name="payrollPeriodId"/>
        </relation>
        </entity>
        
        <entity entity-name="StaffPayrollElements" package-name="org.ofbiz.payroll" title="Staff Payroll Elements Entity">
        <field name="staffPayrollElementsId" type="id-ne"><description>primary sequenced ID</description></field>
        <field name="staffPayrollId" type="id"></field>
        <field name="payrollElementId" type="id"></field>
        <field name="amount" type="currency-amount"></field> 
        <field name="balance" type="currency-amount"></field> 
        <field name="recurrencyExpiry" type="date-time"></field>
        <field name="valueChanged" type="indicator"></field>     
        <prim-key field="staffPayrollElementsId"/>
        <relation type="one" fk-name="payrollelements_period" rel-entity-name="StaffPayroll">
            <key-map field-name="staffPayrollId"/>
        </relation>
        <relation type="one" fk-name="management_element" rel-entity-name="PayrollElement">
            <key-map field-name="payrollElementId"/>
        </relation>
        </entity> 
        
        <!-- The Stations Entity -->
        <entity entity-name="Station" package-name="org.ofbiz.payroll" title="Station Entity">
        	<field name="stationId" type="id-ne"><description>primary sequenced ID</description></field>
        	<field name="isActive" type="indicator"></field>
        	<field name="createdBy" type="name"></field>
        	<field name="updatedBy" type="name"></field>
        	<field name="branchId" type="name"></field>
        	
        	<field name="stationNumber" type="name"></field>
        	
       		<field name="name" type="name"></field>
        	<field name="employerId" type="id"></field>
           
        	<prim-key field="stationId"/>
        	<relation type="one" fk-name="station_employer" rel-entity-name="Employer">
            	<key-map field-name="employerId"/>
        	</relation>
        </entity>
        
        <entity entity-name="PayrollConstants" package-name="org.ofbiz.payroll" title="Payroll Constants Entity">
	        <field name="payrollConstantsId" type="id-ne"><description>primary sequenced ID</description></field>
	        <field name="disability_relief" type="currency-amount"></field>
	        <field name="lowInterstRate" type="numeric"></field> 
	        <field name="pension_maxContibution" type="currency-amount"></field> 
	        <field name="monthlyPersonalRelief" type="currency-amount"></field>
	        <field name="insuranceReliefmax" type="currency-amount"></field>
	        <field name="insuranceReliefRate" type="floating-point"></field>
	        <field name="staticnssfAmount" type="currency-amount"></field>
	        
	        <prim-key field="payrollConstantsId"/>
        </entity>
        
         <entity entity-name="PAYETable" package-name="org.ofbiz.payroll" title="PAYE Table Entity">
	        <field name="payeTableId" type="id-ne"><description>primary sequenced ID</description></field>
	        <field name="lowerbracket" type="currency-amount"></field>
	        <field name="upperbracket" type="currency-amount"></field>
	        <field name="percentage" type="floating-point"></field> 
	        <field name="amount" type="currency-amount"></field> 
	        <prim-key field="payeTableId"/>
        </entity>
        
        <entity entity-name="NHIFTable" package-name="org.ofbiz.payroll" title="NHIF Table Entity">
	        <field name="nhifTableId" type="id-ne"><description>primary sequenced ID</description></field>
	        <field name="lowerbracket" type="currency-amount"></field>
	        <field name="upperbracket" type="currency-amount"></field>
	        <field name="contribution" type="currency-amount"></field> 
	        <prim-key field="nhifTableId"/>
        </entity>
        
        <entity entity-name="NSSFTable" package-name="org.ofbiz.payroll" title="NSSF Table Entity">
	        <field name="nssfTableId" type="id-ne"><description>primary sequenced ID</description></field>
	        <field name="tiername" type="currency-amount"></field>
	        <field name="upperbracket" type="currency-amount"></field>
	        <field name="percentage" type="floating-point"></field> 
	        <prim-key field="nssfTableId"/>
        </entity>
        
        
        
            
	
</entitymodel>
