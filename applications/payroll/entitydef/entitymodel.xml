    	<member-entity entity-alias="PE" entity-name="PayrollElement" />
        <alias-all entity-alias="SPE"/>
        <alias-all entity-alias="PE" />
        <view-link entity-alias="SPE" rel-entity-alias="PE">
            <key-map field-name="payrollElementId" />
        </view-link>
        <entity-condition>
        	<condition-expr entity-alias="SPE" field-name="amount" operator="greater" value="0"/>
